{
	"name":"ignition-dsa",
	"version":"0.1.0-SNAPSHOT",
	"description":"Spark DSLink",
	"license":"Apache",
	"author":{
		"name":"Vlad Orzhekhovskiy",
		"email":"vlad@uralian.com"
	},
	"main":"bin/ignition",
	"repository":{
		"type":"git",
		"url":"https://github.com/IOT-DSA/sdk-spark-dslink-scala"
	},
	"bugs":{
		"url":"https://github.com/IOT-DSA/sdk-spark-dslink-scala/issues"
	},
	"configs":{
		"name":{
			"type":"string",
			"default":"ignition"
		},
		"broker":{
			"type":"url"
		},
		"token":{
			"type":"string"
		},
		"nodes":{
			"type":"path",
			"default":"nodes.json"
		},
		"key":{
			"type":"path",
			"default":".key"
		},
		"log":{
			"type":"enum",
			"default":"info"
		},
		"handler_class":{
			"type":"string",
			"default":"org.dsa.iot.spark.DSAConnector$"
		}		
	}
}